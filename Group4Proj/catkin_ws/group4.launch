<launch>
  
  <node name="rviz" pkg="rviz" type="rviz">
  </node>

  <node name="RosAria" pkg="rosaria" type="RosAria">
    <param name="port" value="/dev/ttyS1" />
  </node>
  
  <node name="Joy" pkg="joy" type="joy_node">
  </node>

  <node name="GPS" pkg="nmea_navsat_driver" type="nmea_serial_driver">
    <param name="port" value="/dev/ttyACM0" />
    <param name="baud" value="9600" />
  </node>

  <node name="GPS_Travel" pkg="gps_travel" type="GPSTravel.py">
  </node>
  
  <node name="MegaMind" pkg="megamind" type="MegaMind.py">
  </node>

  <node name="GUI" pkg="gui" type="GUI.py">
  </node>

</launch>
